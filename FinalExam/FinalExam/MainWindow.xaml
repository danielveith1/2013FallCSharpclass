<Window x:Class="FinalExam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:toolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Input.Toolkit"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        
        <StackPanel>
            <TextBlock Text="Name a City." />
            <toolkit:AutoCompleteBox
              ToolTip="Name a City"
              x:Name="City" Height="30" Width="100"
              Populating="City_Populating" />
        </StackPanel>
        <DataGrid HorizontalAlignment="Left" Margin="566,70,-149,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
   
</Window>
